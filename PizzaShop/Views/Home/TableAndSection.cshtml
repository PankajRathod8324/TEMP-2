@{
    ViewData["Title"] = "Table And Section Page";
    @if (TempData["Message"] != null)
    {
        @TempData["Message"]
    }
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User Page</title>
    <link rel="stylesheet" href="~/css/style.css">
    <link rel="stylesheet" href="~/css/media.css">
    <link rel='stylesheet' href='https://cdn.datatables.net/1.13.5/css/dataTables.bootstrap5.min.css'>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.7.2/css/all.min.css"
        integrity="sha512-Evv84Mr4kqVGRNSgIGL/F/aIDqQb7xQ2vcrdIwxfjThSH8CSR7PBEakCr51Ck+w+/U6swU2Im1vVX0SVk9ABhg=="
        crossorigin="anonymous" referrerpolicy="no-referrer" />
</head>

<body class="bg-body-tertiary">

    <!-- Overlay for small screens -->
    <div class="overlay" id="overlay"></div>


    <div class="content " id="content">
        <!-- Main content goes here -->
        <div class="row d-flex mb-2">
            <div class="row">
                <div class="col-6 ms-4">
                    <h1 class=" d-flex justify-content-start webybtn">Sections/Tables </h1>

                </div>
                <div class="col d-flex justify-content-end">
                    <a asp-action="" asp-controller="" asp-action="">
                        <button class="btn btn-outline-primary  " type="submit"> <i
                                class="fa-solid fa-chevron-left"></i>Back</button>
                    </a>
                </div>
            </div>
        </div>
        <div class="row mainbody d-flex mb-2 mt-2 ms-3 mt-4 pt-3 pe-0">
            <div class="bg-white p-3">

                <div id="sectionbody">

                </div>


                <div id="tablebody">
                </div>



            </div>
        </div>


    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
        crossorigin="anonymous"></script>

    <!-- jQuery -->
    <script src='https://code.jquery.com/jquery-3.7.0.js'></script>

    <!-- JS for sidebar toggle -->

    <script src="~/js/userPage.js"></script>
</body>

<script>
    $(document).ready(function () {
        console.log("Hiiii");
        $.ajax({
            url: '@Url.Action("Section", "TableAndSection")',
            type: 'GET',
            success: function (data) {
                $('#sectionbody').append(data);
            },
            error: function (xhr, status, error) {
                console.error('error Feching items:', error);
            }
        });
    });

    @* $(document).ready(function () {
        $.ajax({
            url: '@Url.Action("MenuModifierGroup", "Menu")',
            type: 'GET',
            success: function (data) {
                $('#myTab2').append(data);
            },
            error: function (xhr, status, error) {
                console.error('error Feching items:', error);
            }
        });
    }); *@

</script>

</html>